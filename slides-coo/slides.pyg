  class Bicicleta {
    int cadência = 0;
    int velocidade = 0;
    int marcha = 1;

    void mudarCadência(int novoValor) {
         cadência = novoValor;
    }
    void mudarMarcha(int novoValor) {
         marcha = novoValor;
    }
    void acelerar(int incremento) {
         velocidade = velocidade + incremento;
    }
    void frear(int decremento) {
         velocidade = velocidade - decremento;
    }
}
